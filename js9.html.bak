<!DOCTYPE html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <title>Document</title>
 </head>
 <body>
  <script>
x=4;
y=6;
//arithmatic		+      -       *       /        %         **
document.write(x+y,"<br>")
document.write(x-y,"<br>")
document.write(x*y,"<br>")
document.write(x/y,"<br>")
document.write(x%y,"<br>")
//relational    >      <       >=      <=      ==       !=    
//  ===       !==
document.write(x>0,"<br>")
document.write(x==0,"<br>")
document.write(x!=y,"<br>")
document.write(x!==y,"<br>")
document.write(x===y,"<br>")
//logical			&&		||			!
document.write(x<0 &&y<0,"<br>")
document.write( x>=1 && x<=100 , `<br>`)
document.write( y===1 || y===7 || y===10, `<br>`)
x+=10;
document.write(x,"<br>")
y-=2;
document.write(y,"<br>")
x*=3;
document.write(x,"<br>")
document.write(`nes`+`  swam`+"<br>")
x++;
document.write(x,"<br>")

//ternary		?:
			document.write( y%2===0 ? "Even number" : "Odd number");

</script>
 </body>
</html>
