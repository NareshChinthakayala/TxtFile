<!DOCTYPE html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <title>Document</title>
 </head>
 <body>
<h3>finding max and min values</h3>
  <script>
let a=[];
let n=+prompt("enter number");
for (let i=0;i<n ;i++ )
{
a[i]=+prompt(`enter a$[i] value`);
document.write(`${a[i]}`);
}
let max=a[0];
let min=a[0];

for(let i=1; i<n; i++)
		{
			if(a[i]>max)
				max=a[i];

			if(a[i]<min)
				min=a[i];
		}document.write(`${a[i]}`)
document.write(`Max Ele		:${max}<br>`);
		document.write(`Min Ele		:${min}<br>`);

</script>
 </body>
</html>
